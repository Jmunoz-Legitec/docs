            "pages": [
              "PruebasTests/Whitelisting/Google Whitelisting",
              "PruebasTests/Whitelisting/Microsoft M365 Whitelisting"
            ]
          },
          {
            "group": "Noticias",
            "pages": [
              "PruebasTests/Noticias/Noticias Destacadas"
            ]
          },
          {
            "group": "Precios",
            "pages": [
              "PruebasTests/Precios/Basico",
              "PruebasTests/Precios/Premium"
            ]
          }
